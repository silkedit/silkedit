{
  "source": {
      "exclude": [ "jslib/node_modules/silkedit/node_modules" ],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "logoFile": "",
    "cleverLinks": true,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": false,
    "outputSourcePath": false,
    "systemName": "SilkEdit",
    "footer": "",
    "copyright": "Copyright (c) 2016 SilkEdit All Rights Reserved.",
    "navType": "vertical",
    "theme": "cosmo",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": true,
    "protocol": "html://",
    "methodHeadingReturns": false,
    "analytics": {"ua":"UA-77280714-1", "domain":"silkedit.io"}
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}
